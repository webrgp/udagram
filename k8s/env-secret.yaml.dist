apiVersion: v1
kind: Secret
metadata:
  name: env-secret
type: Opaque
stringData:
  JWT_SECRET: {{JWT_SECRET}}
  POSTGRES_USERNAME: {{POSTGRES_USERNAME}}
  POSTGRES_PASSWORD: {{POSTGRES_PASSWORD}}
  POSTGRES_HOST: {{POSTGRES_HOST}}
  AWS_BUCKET: {{AWS_BUCKET}}
  AWS_REGION: {{AWS_REGION}}
  AWS_PROFILE: {{AWS_PROFILE}}